{
  "hosting": {
    "public": "public",
    "cleanUrls": "true",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ {
      "source" : "**/*.@(eot|otf|ttf|ttc|woff|font.css)",
      "headers" : [ {
        "key" : "Access-Control-Allow-Origin",
        "value" : "*"
      } ]
    }, {
      "source" : "**/*.@(jpg|jpeg|gif|png|svg|)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=31536000"
      } ]
    }, 
    {
      "source" : "**/*.@(css|js|`)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=1209600"
      } ]
    } ],
    "redirects": [ {
      "source" : "/registration",
      "destination" : "/our-center",
      "type" : 301
    }, {
      "source" : "/aboutus",
      "destination" : "/",
      "type" : 301
    }, {
      "source" : "/gallery",
      "destination" : "/our-center",
      "type" : 301
    }, {
      "source" : "/tuition",
      "destination" : "/prices",
      "type" : 301
    }, {
      "source" : "/blog",
      "destination" : "/news/overview",
      "type" : 301
    }, {
      "source" : "/staff",
      "destination" : "/careers",
      "type" : 301
    }, {
      "source" : "/news/overview/category/Announcements",
      "destination" : "/news/overview",
      "type" : 301
    } ]
  }
}
